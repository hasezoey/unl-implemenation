{
	"name": "unl-implementation",
	"version": "0.0.1",
	"description": "UnNamed Language Implementation",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"engines": {
		"node": ">=12.0.0"
	},
	"files": [
		"lib/**/*.js",
		"lib/**/*.d.ts"
	],
	"scripts": {
		"start": "npm run build && npm run run",
		"run": "node build/src/run.js",
		"watch": "tsc -w",
		"build": "tsc",
		"lint": "tslint -c tslint.json -p tsconfig.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --collectCoverage",
		"test:watchCoverage": "jest --watch --collectCoverage",
		"doc": "typedoc --out ./doc ./src --mode modules",
		"clean": "rm -rf ./doc ./lib ./coverage ./nyc_output",
		"prepare": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "none"
	},
	"author": "hasezoey",
	"license": "MIT",
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^0.1.3",
		"@types/debug": "^4.1.5",
		"@types/jest": "^24.0.22",
		"@types/node": "^12.12.6",
		"@types/source-map-support": "^0.5.0",
		"jest": "^24.9.0",
		"jest-extended": "^0.11.2",
		"nyc": "^14.1.1",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.4.1",
		"tslint": "^5.20.1",
		"tslint-eslint-rules": "^5.4.0",
		"typedoc": "^0.15.0",
		"typescript": "^3.7.2"
	},
	"dependencies": {
		"loglevel": "^1.6.4",
		"source-map-support": "^0.5.16"
	},
	"publishConfig": {
		"access": "public"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "",
		"testRegex": ".test.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "./coverage",
		"testEnvironment": "node",
		"setupFilesAfterEnv": [
			"jest-extended"
		],
		"globals": {
			"ts-jest": {
				"diagnostics": true,
				"disableSourceMapSupport": true
			}
		},
		"collectCoverage": false,
		"collectCoverageFrom": [
			"**/*{.ts}",
			"**/src/**",
			"!**/*{.d.ts,.js}"
		]
	}
}
